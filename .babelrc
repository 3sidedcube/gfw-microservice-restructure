{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "targets": {
          "node": "12.22.11"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "module-resolver",
      {
        "root": ["./app/src"],
        "extensions": [".js", ".ts"],
        "alias": {
          "@routes": "../build/routes"
        }
      }
    ]
  ]
}